    <TextView
        android:id="@+id/firstName3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="2dp"
        android:text="Tech3 Name "
        android:textColor="#15319E"
        android:textSize="20sp"
        app:fontFamily="@font/cardo_bold"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/live_back_card"
        app:layout_constraintTop_toBottomOf="@+id/contactdisplay_div3" />

    <TextView
