        android:layout_marginTop="4dp"
        android:text="--------"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/phone_text2"
        app:layout_constraintTop_toBottomOf="@+id/firstName2" />

    <TextView
        android:id="@+id/email_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/cardo_bold"
        android:text="Email:"
        android:textColor="#BC12172A"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/phone_text2"
        app:layout_constraintTop_toBottomOf="@+id/phone_text2" />

    <TextView
        android:id="@+id/email_show2"
        android:layout_width="wrap_content"
