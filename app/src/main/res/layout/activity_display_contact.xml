<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    android:paddingLeft="16dp"
    android:paddingTop="16dp"
    android:paddingRight="16dp"
    android:paddingBottom="16dp"
    tools:context=".LiveActivity">


    <View
        android:id="@+id/live_back_card"
        android:layout_width="380dp"
        android:layout_height="680dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.458"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/contactdisplay_div1"
        android:layout_width="380dp"
        android:layout_height="5dp"
        android:layout_marginTop="16dp"
        android:background="#009688"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title" />

    <View
        android:id="@+id/contactdisplay_div2"
        android:layout_width="380dp"
        android:layout_height="5dp"
        android:layout_marginTop="8dp"
        android:background="#009688"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_text" />

    <View
        android:id="@+id/contactdisplay_div3"
        android:layout_width="380dp"
        android:layout_height="5dp"
        android:layout_marginTop="8dp"
        android:background="#009688"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_text2" />

    <View
        android:id="@+id/contactdisplay_div4"
        android:layout_width="380dp"
        android:layout_height="5dp"
        android:layout_marginTop="8dp"
        android:background="#009688"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_text3" />

    <View
        android:id="@+id/contactdisplay_div5"
        android:layout_width="380dp"
        android:layout_height="5dp"
        android:layout_marginTop="16dp"
        android:background="#009688"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_text4" />

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Saved Contacts"
        android:textColor="#138CA1"
        android:textSize="32sp"
        app:fontFamily="@font/cardo_bold"
        app:layout_constraintEnd_toEndOf="@+id/live_back_card"
        app:layout_constraintStart_toStartOf="@+id/live_back_card"
        app:layout_constraintTop_toTopOf="@+id/live_back_card" />


    <TextView
        android:id="@+id/firstName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="16dp"
        android:text="Tech1 Name "
        android:textColor="#15319E"
        android:textSize="20sp"
        app:fontFamily="@font/cardo_bold"
        app:layout_constraintBottom_toTopOf="@+id/phone_text"
        app:layout_constraintStart_toStartOf="@+id/live_back_card"
        app:layout_constraintTop_toBottomOf="@+id/contactdisplay_div1" />

    <Button
        android:id="@+id/getcontacts_button"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/gradient"
        android:padding="14dp"
        android:text="Load Contacts"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        app:fontFamily="@font/cardo_bold"
        app:layout_constraintBottom_toBottomOf="@+id/live_back_card"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contactdisplay_div5" />

    <TextView
        android:id="@+id/lastName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Last Name"
        android:textColor="#15319E"
        android:textSize="20sp"
        app:fontFamily="@font/cardo_bold"
        app:layout_constraintStart_toEndOf="@+id/firstName"
        app:layout_constraintTop_toTopOf="@+id/firstName" />

    <TextView
        android:id="@+id/email_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/cardo_bold"
        android:text="Email:"
        android:textColor="#BC12172A"
        android:textSize="20sp"

        app:layout_constraintStart_toStartOf="@+id/phone_text"
        app:layout_constraintTop_toBottomOf="@+id/phone_text" />

    <TextView
        android:id="@+id/phone_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:fontFamily="@font/cardo_bold"
        android:text="Phone:"
        android:textColor="#BC12172A"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/firstName"
        app:layout_constraintTop_toBottomOf="@+id/firstName" />

    <TextView
        android:id="@+id/phone_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="--------"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/phone_text"
        app:layout_constraintTop_toTopOf="@+id/phone_text" />

    <TextView
        android:id="@+id/email_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="-----------"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/email_text"
        app:layout_constraintTop_toTopOf="@+id/email_text" />

    <TextView
        android:id="@+id/firstName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="2dp"
        android:text="Tech2 Name "
        android:textColor="#15319E"
        android:textSize="20sp"
        app:fontFamily="@font/cardo_bold"
        app:layout_constraintStart_toStartOf="@+id/live_back_card"
        app:layout_constraintTop_toBottomOf="@+id/contactdisplay_div2" />

    <TextView
        android:id="@+id/lastName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Last Name"
